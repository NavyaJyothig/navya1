#include<stdio.h>


int main()
{
    int a[100],i,n,key;
    printf("\n Enter the  numbers of elements \n ");
    scanf("%d",&n);
    printf("\n Enter %d elements \n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("\n enter the key element \n");
    scanf("%d",&key);
    project(a,n,key);
    return 0;
}
void project(int a[100],int n,int key)
{
    int i;
    for(i=0;i<=n;i++)
    {
        if (key==a[i])
        {
            printf("\n element %d found at %d position \n",key,i+1);
            exit(0);
        }
    }printf("\n element not found \n");
    
}
        
    
