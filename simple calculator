#include <stdio.h>
int input(int *a,int *b)
{
    
    
    printf("enter the values of a and b : ");
    scanf("%d%d",a,b);
    
    return 0;
}
void compute()

{
 int  c,a,b;   
    input(&a,&b);
    int choice;
    printf("enter the choice \n 1.Addition \n 2.Subtraction \n 3.Multiplication \n 4.Division");
    scanf("%d",&choice);
    switch(choice)
    {
        case 1:
        c=a+b;
        printf("Addition %d",c);
        break;
        case 2:
        c=a-b;
        printf("Subtraction %d",c);
        break;
        case 3:
        c=a*b;
        printf("Multiplication %d",c);
    break;
    case 4:
    printf("Division %d",c);
    break;
    default:
    printf("your choice is not valid");
    }
    return 0;
}
int main()
         
         {
             
             compute();
             
         }
